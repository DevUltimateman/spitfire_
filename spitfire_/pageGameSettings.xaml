      d:DesignHeight="450" d:DesignWidth="800"
      Title="pageGameSettings">

    <Grid  Background="Transparent"
          

          >

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
